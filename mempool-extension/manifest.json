{
    "manifest_version": 3,
    "name": "Bitcoin Fee Rates",
    "description": "Shows Bitcoin fee rates using the mempool.space API.",
    "version": "1.0",
    "permissions": [
      "alarms",
      "storage",
      "background"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  